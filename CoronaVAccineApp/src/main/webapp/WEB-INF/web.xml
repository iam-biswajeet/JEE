<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
 <welcome-file-list>
 <welcome-file>servleturl</welcome-file>
	<welcome-file>corona_vaccine.html</welcome-file>
</welcome-file-list> 
  <!-- servlet conf -->
  <servlet>
  <servlet-name>coronaeligibility</servlet-name>
  <servlet-class>com.nt.servlet.VaccineEligibilityServlet</servlet-class>
  </servlet>
  <!-- servlet conf -->
  <servlet-mapping>
  <servlet-name>coronaeligibility</servlet-name>
  <url-pattern>/coronaurl</url-pattern>
  </servlet-mapping>
   <servlet>
  <servlet-name>welcome</servlet-name>
  <servlet-class>com.nt.servlet.WelcomeServlet</servlet-class>
  </servlet>
  <!-- servlet conf -->
  <servlet-mapping>
  <servlet-name>welcome</servlet-name>
  <url-pattern>/servleturl</url-pattern>
  </servlet-mapping>
  
  <!-- servlet conf -->
  
   <servlet>
  <servlet-name>home</servlet-name>
  <jsp-file>/WEB-INF/pages/corona_vaccine.html</jsp-file>
  </servlet>
  <!-- servlet conf -->
  <servlet-mapping>
  <servlet-name>home</servlet-name>
  <url-pattern>/homeurl</url-pattern>
  </servlet-mapping>
   <servlet>
  <servlet-name>index</servlet-name>
  <jsp-file>/index.html</jsp-file>
  </servlet>
  <!-- servlet conf -->
  <servlet-mapping>
  <servlet-name>index</servlet-name>
  <url-pattern>/indexurl</url-pattern>
  </servlet-mapping>
</web-app>